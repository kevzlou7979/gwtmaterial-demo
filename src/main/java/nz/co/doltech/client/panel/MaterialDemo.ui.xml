<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
						 xmlns:g="urn:import:com.google.gwt.user.client.ui"
						 xmlns:m="urn:import:gwt.material.design.client.ui"
						 xmlns:m.html="urn:import:gwt.material.design.client.ui.html"
		>
	<ui:with field='res' type='nz.co.doltech.client.resources.MaterialResources' />
	<ui:style>	
		nav{
			box-shadow: none;
		}
		.button{
			vertical-align: top;
			margin-left: 20px;
		}
		.panel{
			padding-left: 5%;
			width: 100%;
			margin-top: -150px;
			padding: 10px 50px 150px 50px;
			background: #FFF;
			box-sizing: border-box;
			-webkit-box-sizing: border-box;
			-moz-box-sizing: border-box;
		}
		.pointer{
			cursor: pointer;
		}
		main, footer, header{
			padding-left: 280px;
		}
		.footer{
			margin-top: -50px !important;
		}
		.logo{
			padding: 10px;
			border-bottom: 1px solid #E9E9E9;
		}
		.btnGettingStarted{
			font-size: 0.3em !important;
		}
	</ui:style>	
	<g:HTMLPanel >
		  <m:MaterialNavBar ui:field="narBar" backgroundColor="blue darken-1" sideBar="mysidebar" />
				
			<m:MaterialSideNav ui:field="sideNav" width="280" type="OPEN" closeOnClick="false">
					<m.html:ListItem waves="DEFAULT">
			    	<m:MaterialImage resource="{res.ic_gwt_logo}" addStyleNames="{style.logo}"/>
					</m.html:ListItem>
					<m.html:ListItem waves="DEFAULT">
			    	<m:MaterialLink href="#about" iconPosition="LEFT" iconType="INFO_OUTLINE" text="About" textColor="blue" />
					</m.html:ListItem>
					<m.html:ListItem waves="DEFAULT">
						<m:MaterialLink href="#gettingStarted" iconPosition="LEFT" iconType="CLOUD_DOWNLOAD" text="Getting Started" textColor="blue">
							<m:MaterialBadge text="1.4" backgroundColor="blue" textColor="white"/>
						</m:MaterialLink>
					</m.html:ListItem>
				<m:MaterialCollapsible>
				    <!-- COMPONENTS -->
					<m:MaterialCollapsibleItem>
					    <m:MaterialCollapsibleHeader waves="DEFAULT">
					        <m:MaterialLink text="Components" iconType="POLYMER" textColor="blue"/>
					    </m:MaterialCollapsibleHeader>
						<m:MaterialCollapsibleBody>
						    <m.html:UnorderedList>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#autocompletes" text="Autocomplete" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#badges" text="Badges" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#buttons" text="Buttons" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#cards" text="Cards" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#chips" text="Chips" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#collapsibles" text="Collapsible" textColor="blue"/></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#collections" text="Collections" textColor="blue"/></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#datagrid" text="Datagrid" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#dialogs" text="Dialogs" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#dropdowns" text="Dropdown" textColor="blue"/></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#errors" text="Errors" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#forms" text="Forms" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#footer" text="Footer" textColor="blue"/></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#loaders" text="Loaders" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#media" text="Media" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#navigations" text="NavBar" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#pickers" text="Pickers" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#scrollspy" text="Scrollspy" textColor="blue" /></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#tabs" text="Tabs" textColor="blue"/></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#transition" text="Transitions" textColor="blue"/></m.html:ListItem>
							</m.html:UnorderedList>
						</m:MaterialCollapsibleBody>
					</m:MaterialCollapsibleItem>
					<!-- STYLE AND LAYOUTS -->
					<m:MaterialCollapsibleItem>
						<m:MaterialCollapsibleHeader>
							<m:MaterialLink text="Style and Layout" iconType="STYLE" textColor="blue"/>
						</m:MaterialCollapsibleHeader>
						<m:MaterialCollapsibleBody>
							<m.html:UnorderedList>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#colors" text="Colors" textColor="blue"/></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#grid" text="Grid" textColor="blue"/></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#helper" text="Helper" textColor="blue"/></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#icons" text="Icons" textColor="blue"/></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#shadow" text="Shadow" textColor="blue"/></m.html:ListItem>
								<m.html:ListItem waves="DEFAULT"><m:MaterialLink href="#theme" text="Theming" textColor="blue"/></m.html:ListItem>
							</m.html:UnorderedList>
						</m:MaterialCollapsibleBody>
					</m:MaterialCollapsibleItem>
				</m:MaterialCollapsible>
				<!-- SHOWCASE -->
				<m:MaterialLink href="#showcase" iconPosition="LEFT" iconType="WEB" text="Showcase" textColor="blue" />
				<!-- TEMPLATES -->
				<m:MaterialLink href="#templates" iconPosition="LEFT" iconType="DASHBOARD" text="Templates" textColor="blue" />
				<!-- CHARTS -->
				<m:MaterialLink href="#charts" iconPosition="LEFT" iconType="INSERT_CHART" text="Material Charts" textColor="blue" />
			</m:MaterialSideNav>
	    
			<m:MaterialContainer>
				<m:MaterialTopNav ui:field="topNav" backgroundColor="blue" textColor="white">
					<m:child>
						<m:MaterialPanel>
							<m:MaterialButton ui:field="gettingStartedPanel" text="Get Started" size="LARGE" backgroundColor="blue lighten-2" addStyleNames="{style.btnGettingStarted}"/>
						</m:MaterialPanel>
					</m:child>
						
				</m:MaterialTopNav>
				<g:HTMLPanel ui:field="contentPanel" addStyleNames="{style.panel} container z-depth-2" />
				<m:MaterialPanel>
					<g:HTMLPanel>
				    <input type="hidden" name="IL_IN_TAG" value="1"/>
					</g:HTMLPanel>
				</m:MaterialPanel>
			</m:MaterialContainer>

			<m:MaterialFooter backgroundColor="blue" addStyleNames="{style.footer}">
					<m:MaterialRow>
						<m:MaterialColumn grid="s12 m4 m4">
							<g:HTMLPanel>
								<m:MaterialTitle fontSize="0.7em" color="white" title="Support Us" description="Any amount would help support and continue development on this project and is greatly appreciated."/>
								<form action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_top">
								<input type="hidden" name="cmd" value="_s-xclick"/>
								<input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----
								"/>
								<input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_donateCC_LG.gif" border="0" name="submit" alt="PayPal - The safer, easier way to pay online!"/>
								<img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1"/>
								</form>
								<m:MaterialLabel textColor="white" text="For bug bounties:"/>
								<m:MaterialButton ui:field="btnBounty" text="Bountysource" backgroundColor="green" waves="LIGHT"/>
							</g:HTMLPanel>
						</m:MaterialColumn>
						<m:MaterialColumn grid="s12 m4 l4">
							<m:MaterialTitle fontSize="0.7em" color="white" title="Join The Discussion" description="We provide Gitter Chat rooms in order for GWT Developers discussed and collaborate about GWT Material Design and Phonegap Integration."/>
							<m:MaterialButton ui:field="btnChat" text="CHAT" backgroundColor="blue lighten-2" waves="LIGHT"/>
						</m:MaterialColumn>
						<m:MaterialColumn grid="s12 m4 l4">
							<m:MaterialTitle fontSize="0.7em" color="white" title="Github" description="Download, fork, pull, file issues, and more at the official GWT Material repo on Github."/>
							<m:MaterialButton ui:field="btnGithub" text="Github" backgroundColor="blue lighten-2" waves="LIGHT"/>
						</m:MaterialColumn>
					</m:MaterialRow>
			</m:MaterialFooter>
		
	</g:HTMLPanel>
</ui:UiBinder> 
